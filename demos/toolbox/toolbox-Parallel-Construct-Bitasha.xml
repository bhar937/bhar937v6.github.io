<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">
  <block type="factory_base" deletable="false" movable="false">
    <mutation connections="NONE"></mutation>
    <field name="NAME">Parallel Construct</field>
    <field name="INLINE">AUTO</field>
    <field name="CONNECTIONS">NONE</field>
    <statement name="INPUTS">
      <block type="input_value">
        <field name="INPUTNAME">NAME</field>
        <field name="ALIGN">LEFT</field>
        <statement name="FIELDS">
          <block type="field_static">
            <field name="TEXT">Parallelize</field>
            <next>
              <block type="field_input">
                <field name="TEXT">Static</field>
                <field name="FIELDNAME">NAME</field>
              </block>
            </next>
          </block>
        </statement>
        <value name="TYPE">
          <shadow type="type_null"></shadow>
        </value>
      </block>
    </statement>
    <value name="TOOLTIP">
      <block type="text" deletable="false" movable="false">
        <field name="TEXT"></field>
      </block>
    </value>
    <value name="HELPURL">
      <block type="text" deletable="false" movable="false">
        <field name="TEXT"></field>
      </block>
    </value>
    <value name="COLOUR">
      <block type="colour_hue">
        <mutation colour="#a5805b"></mutation>
        <field name="HUE">30</field>
      </block>
    </value>
  </block>
  <block type="parallel_construct">
    <field name="NAME">Static</field>
  </block>
</xml>